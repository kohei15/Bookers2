<h1>Editing Book</h1>
<%= form_for(@book) do |f| %>
	<label for="book_title">Title</label>
	<%= f.text_field :title,class: "book_title" %>
	<label for="book_body">Body</label>
	<%= f.text_area :body,class: "book_body}" %>
	<br><%= f.submit 'Update Book' %>
<% end %>
<%= link_to "Show", book_path(@book.id),class: "show_#{@book.id}" %> | <%= link_to "Back", books_path,class: "back" %>